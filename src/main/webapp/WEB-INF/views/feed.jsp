<div class="connUs-feed-wrapper">
	<div class="connUs-addFeed-cont">
		<textarea class="connUs-addFeed-desc" type="text" placeholder="Feed Anything" data-emojiable="true"/></textarea>
		<div class="connUs-feed-smiley"></div>
		<div class="connUs-youtubeAddCont">
			<input type="text" class="youtube-url" placeholder="Youtube Url"/>
		</div>
		<div class="feed-youtubeVideo-display"></div>
		<div class="feed-addOptionCont">
			<form id="connUs-addFeed-img">
			    <input id="connUs-feed-imgUpload" type="file" name="file" />
			</form>
			<div class="add-youtubeIcon"></div>
			</div>
		    <div class="connUs-addFeedImgDisplayCont">
		        <img class="connUs-addFeedImgDisplay"/>
		    </div>
		    <div class="connUs-addFeed-btn">Feed</div>
	</div>
	<div class="connUs-feedList-wrapper"></div>
</div>
